  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Network" ],
    "MinimumLevel": "Error",
    "WriteTo": [
      {
        "Name": "TCPSink",
        "Args": { "uri": "tls://localhost:5600" }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "elasticsearch": {
    "index":  "elk" ,
    "url": "http://localhost:9600/"
  }
}
